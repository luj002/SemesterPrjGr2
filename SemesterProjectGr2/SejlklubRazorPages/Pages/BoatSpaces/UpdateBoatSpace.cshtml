@page
@model SejlklubRazorPages.Pages.BoatSpaces.UpdateBoatSpaceModel
@{
    ViewData["Title"] = "Update boat space";
}
<h1>Update boat space</h1>
<div class="row">
    <div class="col-md-4">
        <form method="post">

            <input type="hidden" asp-for="@Model.BoatSpace.Number" />
            <div class="form-group">
                <label asp-for="@Model.BoatSpace.Number" class="control-label"></label>
                <input asp-for="@Model.BoatSpace.Number" class="form-control" />

            </div>
            <div class="formgroup">
                <input type="submit" asp-page-handler="Update" value="Update" class="btn btn-primary" />
            </div>
            <div class="formgroup">
                <input type="submit" asp-page-handler="Remove" value="Remove" class="btn btn-danger" />
            </div>


        </form>


    </div>
    <div>
        <a asp-page="Index">Back to List</a>
    </div>
</div>